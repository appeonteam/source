this.Item[UpperBound(this.Item)+1]=this.m_remote
this.Item[UpperBound(this.Item)+1]=this.m_help
end on

on m_main.destroy
call super::destroy
destroy(this.m_file)
destroy(this.m_profiles)
destroy(this.m_local)
destroy(this.m_remote)
destroy(this.m_help)
end on

